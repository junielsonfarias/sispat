    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint src --cache",
    "lint:fix": "eslint src --fix --cache",
    "format": "prettier --write --cache .",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "preinstall": "npx only-allow pnpm",
    "test": "vitest --run --cache",
    "test:watch": "vitest",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:build": "pnpm run clean && pnpm run build:prod",
    "deploy:check": "pnpm run lint && pnpm run test"
